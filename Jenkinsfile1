pipeline {
    agent any
    stages{
        stage('Build'){
            steps{
             echo 'Build'
            }
        }
        stage('Test'){
            steps{
              echo 'Test'
            }
        }
        stage('Quality Gate - sonar Quebe'){
            steps{
               echo 'sonar'
            }
        }
        stage('Push to atrifactory'){
            steps{
                echo 'Build'
            }
        }


        stage('Deploy to Dev'){
            steps{
                 echo 'Deploy Dev'
            }
        }
        stage('Deploy to QA'){
            steps{
                 echo 'Deploy QA'
            }
        }
        stage('Deploy to UAT'){
            steps{
                echo 'Deploy UAT'
            } 
        }
         stage('Deploy to Staging'){
            steps{
                echo 'Deploy Stage'
            }
        }
         stage('Deploy to Prod'){
            steps{
               echo 'Deploy Prod'
            } 
        }

        
    }
    post{
            failure{
                echo 'fail'
            }
            
            success{
                echo 'Success'
            }
            
            always{
                echo 'always'
            }
        }
} 